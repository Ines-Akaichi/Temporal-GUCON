PREFIX ex:  <http://example.com/ns#> 
PREFIX hc: <http://wu.ac.at/domain/hospital-inpatient-care#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX gucon: <http://www.wu.ac.at/2024/gucon#>


# Patient
ex:alice-waltz a hc:Patient .


# Admission
ex:adm789 a hc:Admission ;
    hc:hasPatient ex:alice-waltz ;
    hc:hasDischargeForm ex:form321 ;
    hc:hasExpectedAdmissionEndDate "2025-08-15T17:00:00.000+02:00"^^xsd:dateTime .

# Discharge Form
ex:form321 a hc:DischargeForm .

# Action 
<<ex:alice-waltz gucon:sign  ex:form321>> gucon:executionTime  "2025-08-15T15:00:00.000+02:00"^^xsd:dateTime .
