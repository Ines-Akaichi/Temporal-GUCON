PREFIX ex: <http://example.com/ns#>
PREFIX hc:  <http://wu.ac.at/domain/hospital-inpatient-care#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 

# Doctor
ex:angelika-smith a hc:Doctor .

# Patient
ex:alice-waltz a hc:Patient ;
    hc:hasResponsibleDoctor ex:angelika-smith .

# Lab Test
ex:lab123 a hc:LabTest ;
    hc:hasPatient ex:alice-waltz ;
    hc:hasLabResult ex:result123 .

# Lab Result
ex:result123 a hc:LabResult ;
    hc:hasLabResultDatetime "2025-08-10T09:30:00.000+02:00"^^xsd:dateTime .

# Treatment Plan
ex:plan456 a hc:TreatmentPlan ;
    hc:isBasedOnLabResult ex:result123 .

